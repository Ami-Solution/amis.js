{
  "name": "detherjs",
  "version": "0.1.2",
  "description": "Dether SDK to easily call smart contract from Javascript",
  "main": "lib/index.js",
  "scripts": {
    "babel-node": "babel-node --presets=latest",
    "example": "npm run babel-node -- examples/usage.js",
    "test": "mocha --compilers js:babel-core/register test/test.config.js test/specs/**/*.js",
    "prepublish": "babel src --out-dir lib",
    "build": "babel src --out-dir lib",
    "esdoc": "./node_modules/.bin/esdoc && open ./esdoc/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dethertech/dether.js.git"
  },
  "keywords": [
    "Dether",
    "ethereum",
    "sdk"
  ],
  "author": "Dether Team <hello@dether.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dethertech/dether.js/issues"
  },
  "homepage": "https://github.com/dethertech/dether.js#readme",
  "dependencies": {
    "bignumber.js": "^4.1.0",
    "dethercontract": "^1.2.5",
    "eth-toolbox": "^0.2.3",
    "ethers": "^2.1.2",
    "truffle-contract": "^2.0.1",
    "web3": "^0.19.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "esdoc": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.5.3",
    "sinon": "^4.0.2"
  }
}
